{
  "release-type": "simple",
  "packages": {
    ".": {
      "extra-files": [
        {
          "type": "generic",
          "path": "cmd/api/main.go",
          "regex": "const version = \"[0-9]+\\.[0-9]+\\.[0-9]+\"",
          "replacement": "const version = \"${version}\""
        }
      ]
    }
  }
}
